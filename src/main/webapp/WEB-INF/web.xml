    </servlet>
     <servlet>
        <servlet-name>BookUpdateServlet</servlet-name>
        <servlet-class>com.online.bookstore.servlet.BookUpdateServlet</servlet-class>
    </servlet>
     <servlet>
        <servlet-name>YourProfileServlet</servlet-name>
        <servlet-class>com.online.bookstore.servlet.YourProfileServlet</servlet-class>
    </servlet>
     <servlet>
        <servlet-name>Edit_returnAjax</servlet-name>
        <servlet-class>com.online.bookstore.servlet.Edit_returnAjax</servlet-class>
    </servlet>
     <servlet>
        <servlet-name>ImageDisplay</servlet-name>
        <servlet-class>com.online.bookstore.servlet.ImageDisplay</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
   <servlet-mapping>
        <servlet-name>Registration</servlet-name>
        <url-pattern>/Registration</url-pattern>
    </servlet-mapping>
   <servlet-mapping>
        <servlet-name>AddBookServlet</servlet-name>
        <url-pattern>/AddBookServlet</url-pattern>
    </servlet-mapping>
   <servlet-mapping>
        <servlet-name>DeleteBookServlet</servlet-name>
        <url-pattern>/DeleteBookServlet</url-pattern>
    </servlet-mapping>
