        <servlet-name>ViewBookServlet</servlet-name>
        <url-pattern>/ViewBookServlet</url-pattern>
    </servlet-mapping>
   <servlet-mapping>
        <servlet-name>UpdateBookGetData</servlet-name>
        <url-pattern>/UpdateBookGetData</url-pattern>
    </servlet-mapping>
   <servlet-mapping>
        <servlet-name>BuyBookServlet</servlet-name>
        <url-pattern>/BuyBookServlet</url-pattern>
    </servlet-mapping>
   <servlet-mapping>
        <servlet-name>SuccessFullPerches</servlet-name>
        <url-pattern>/SuccessFullPerches</url-pattern>
    </servlet-mapping>
    
