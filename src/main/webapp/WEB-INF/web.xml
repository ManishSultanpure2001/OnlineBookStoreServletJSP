        <servlet-class>com.online.bookstore.servlet.Registration</servlet-class>
    </servlet>
     <servlet>
        <servlet-name>AddBookServlet</servlet-name>
        <servlet-class>com.online.bookstore.servlet.AddBookServlet</servlet-class>
    </servlet>
     <servlet>
        <servlet-name>DeleteBookServlet</servlet-name>
        <servlet-class>com.online.bookstore.servlet.DeleteBookServlet</servlet-class>
    </servlet>
     <servlet>
        <servlet-name>ViewBookServlet</servlet-name>
        <servlet-class>com.online.bookstore.servlet.ViewBookServlet</servlet-class>
    </servlet>
     <servlet>
        <servlet-name>UpdateBookGetData</servlet-name>
        <servlet-class>com.online.bookstore.servlet.UpdateBookGetData</servlet-class>
    </servlet>
     <servlet>
